<!-- Arquivo de configuração para gerar classes a partir de WSDL -->
<jaxb:bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.0"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" jaxb:extensionBindingPrefixes="xjc">
	<jaxb:globalBindings>
		<jaxb:serializable uid="-1" />
		<jaxb:javaType name="java.util.Date" xmlType="xsd:dateTime" parseMethod="com.redhat.DataTypeAdapter.parseDateTime"
			printMethod="com.redhat.DataTypeAdapter.printDateTime" />
		<jaxb:javaType name="java.util.Date" xmlType="xsd:date" parseMethod="com.redhat.DataTypeAdapter.parseDate"
			printMethod="com.redhat.DataTypeAdapter.printDate" />
	</jaxb:globalBindings>
</jaxb:bindings>